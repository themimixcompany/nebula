{
  "name": "mvp",
  "productName": "mvp",
  "version": "1.0.8",
  "description": "Mimix MVP",
  "repository": "https://github.com/themimixcompany/mvp-electron",
  "keywords": [
    "mimix"
  ],
  "author": "The Mimix Company",
  "license": "Blue Oak License 1.0.0",
  "main": "app/main.js",
  "scripts": {
    "start": "electron ."
  },
  "build": {
    "appId": "io.mimix.mvp",
    "productName": "mvp",
    "directories": {
      "output": "./out"
    },
    "linux": {
      "target": [
        "appimage"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "deb": {
      "depends": [
        "gconf2",
        "gconf-service",
        "libnotify4",
        "libappindicator1",
        "libxtst6",
        "libnss3",
        "libgtk-3-0",
        "libnotify4",
        "libnss3",
        "libxss1",
        "libxtst6",
        "xdg-utils",
        "kde-cli-tools",
        "kde-runtime",
        "trash-cli",
        "libglib2.0-bin",
        "gvfs-bin",
        "libasound2"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "express": "^4.17.1",
    "portscanner": "^2.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.45",
    "electron": "7.1.1",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.1.1",
    "electron-prebuilt-compile": "4.0.0"
  }
}
