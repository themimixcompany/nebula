{
    "name": "mimix-mvp",
    "productName": "Mimix MVP",
    "version": "1.0.0",
    "description": "Mimix MVP",
    "main": "main.js",
    "scripts": {
        "engine": "./engine/engine_linux",
        "wwwserver": "ws -d app -s index.html",
        "gui": "electron --no-sandbox .",
        "start": "run-p engine wwwserver gui",
        "package-win": "electron-packager . mvp --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Mimix MVP\"",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
        "package-linux": "electron-packager . mvp --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds"
    },
    "repository": "https://github.com/themimixcompany/mvp",
    "keywords": [
        "Mimix",
        "MVP"
    ],
    "author": "Mimix",
    "license": "Blue Oak License",
    "devDependencies": {
        "electron": "^7.1.1",
        "electron-packager": "^14.1.0",
        "local-web-server": "^3.0.7",
        "npm-run-all": "^4.1.5"
    },
    "dependencies": {}
}
